<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="knack.college.learnenglish.fragments.ShowDictionaryTrainingResultFragment">
    <TextView
        android:id="@+id/complete"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:text="@string/title.you_end_event"
        />
    <TextView
        android:id="@+id/results"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="25sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:text="@string/title.your_results"
        android:layout_below="@id/complete"
        />
    <TextView
        android:id="@+id/condition"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="30sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:layout_below="@id/results"
        />
    <TextView
        android:id="@+id/numberWords"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="42dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="23sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:text="@string/title.wordsNumber"
        android:layout_below="@+id/condition"
        />
    <TextView
        android:id="@+id/numberWordsValue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="23sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:layout_below="@+id/numberWords"
        />
    <TextView
        android:id="@+id/correctAnswer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="23sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:text="@string/title.correct"
        android:layout_below="@+id/numberWordsValue"
        />
    <TextView
        android:id="@+id/correctAnswerValue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="23sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:layout_below="@+id/correctAnswer"
        />
    <TextView
        android:id="@+id/wrongAnswer"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="23sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:text="@string/title.wrong"
        android:layout_below="@+id/correctAnswerValue"
        />
    <TextView
        android:id="@+id/wrongAnswerValue"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="16dp"
        android:layout_marginLeft="16dp"
        android:layout_marginRight="16dp"
        android:textSize="23sp"
        android:textAlignment="center"
        android:textAllCaps="true"
        android:layout_below="@+id/wrongAnswer"
        />
</RelativeLayout>
